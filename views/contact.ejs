<%- include('header') %>

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
            <p>
                简单介绍一下自己啦
            </p>
            <form action="" name="sentMessage" id="contactForm" novalidate>
                <div class="form-group has-success">
                    <label class="control-label" for="name">姓名</label>
                    <input type="text" class="form-control" id="name" aria-describedby="helpBlockName" required data-validation-required-message="请输入名字">
                    <span id="helpBlockName" class="help-block">block</span>
                </div>
                <div class="form-group has-warning">
                    <label class="control-label" for="email">邮箱</label>
                    <input type="email" class="form-control" id="email" aria-describedby="helpBlockEmail" required data-validation-required-message="请输入邮箱">
                    <span id="helpBlockEmail" class="help-block">block</span>
                </div>
                <div class="form-group has-error">
                    <label class="control-label" for="phone">电话号码</label>
                    <input type="tel" class="form-control" id="phone" aria-describedby="helpBlockPhone" required data-validation-required-message="请输入电话号码">
                    <span id="helpBlockPhone" class="help-block">block</span>
                </div>
                <div class="form-group has-error">
                    <label class="control-label" for="messgae">消息</label>
                    <textarea class="form-control" rows="3" id="messgae" aria-describedby="helpBlockMessage" required data-validation-required-message="请输入消息"></textarea>
                    <span id="helpBlockMessage" class="help-block">block</span>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-info">发送</button>
                </div>
            </form>
        </div>
    </div>
</div>

<script src="/lib/jqBootstrapValidation.js"></script>
<script src="/src/component/contack-check.js"></script>
<%- include('footer') %>